{
    "name": "sibyl-timer",
    "author": "Benjamin Sterling",
    "description": "A timer app for TeamWork. Just add API key.",
    "version": "1.0.0",
    "private": true,
    "homepage": "./",
    "main": "public/electron.js",
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "electron-dev": "concurrently \"set BROWSER=none&&npm start\" \"wait-on http://localhost:3000 && electron .\"",
        "preelectron-pack": "npm run build",
        "electron-pack": "electron-builder build",
        "prerelease": "npm run build",
        "release": "electron-builder -p always"
    },
    "devDependencies": {
        "babel-plugin-styled-components": "^1.10.6",
        "concurrently": "^5.0.0",
        "electron": "^11.5.0",
        "electron-builder": "^21.2.0",
        "eslint-plugin-import": "^2.18.2",
        "typescript": "^3.7.2",
        "wait-on": "^3.3.0"
    },
    "dependencies": {
        "@reach/dialog": "^0.6.1",
        "electron-is-dev": "^1.1.0",
        "node-notifier": "^8.0.1",
        "react": "^16.12.0",
        "react-beautiful-dnd": "^12.1.1",
        "react-contenteditable": "^3.3.2",
        "react-contextmenu": "^2.13.0",
        "react-dom": "^16.12.0",
        "react-modal": "^3.11.1",
        "react-redux": "^7.1.3",
        "react-router-dom": "^5.1.2",
        "react-scripts": "3.2.0",
        "react-select": "^3.0.8",
        "react-uid": "^2.2.0",
        "redux": "^4.0.4",
        "redux-persist": "^6.0.0",
        "redux-thunk": "^2.3.0",
        "styled-components": "^4.4.1",
        "uuid": "^3.3.3"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "build": {
        "appId": "com.example.sibyl-timer"
    },
    "babelMacros": {
        "styledComponents": {}
    }
}
